{
  "name": "discordjsgui",
  "version": "1.0.0",
  "description": "A GUI to manage a Discord Bot",
  "main": "main.js",
  "scripts": {
    "dev:react": "webpack --mode development --watch",
    "dev:electron": "wait-on dist/bundle.js && npx electron .",
    "start": "concurrently \"npm:dev:react\" \"npm:dev:electron\"",
    "build:react": "webpack --mode production",
    "build": "npm run build:react && electron-builder"
  },
  "keywords": [],
  "author": "fakelag28",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "axios": "^1.6.8",
    "discord.js": "^14.14.1",
    "html-webpack-plugin": "^5.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "electron": "^37.2.4",
    "electron-builder": "^24.13.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.example.discordjsgui",
    "productName": "DiscordJS GUI",
    "files": [
      "main.js",
      "preload.js",
      "dist/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "dist/icon.ico"
    }
  }
}
